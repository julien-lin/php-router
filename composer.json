{
    "name": "julienlinard/php-router",
    "description": "Un routeur PHP moderne et complet avec support des routes dynamiques, middlewares, groupes de routes et génération d'URL. Compatible PHP 8+ avec Attributes.",
    "type": "library",
    "keywords": [
        "router",
        "routing",
        "php",
        "php8",
        "middleware",
        "http",
        "routes",
        "attributes",
        "psr-4"
    ],
    "homepage": "https://github.com/julien-lin/php-router",
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Linard",
            "email": "julien.linard.dev@gmail.com",
            "homepage": "https://github.com/julien-lin"
        }
    ],
    "support": {
        "issues": "https://github.com/julien-lin/php-router/issues",
        "source": "https://github.com/julien-lin/php-router"
    },
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "JulienLinard\\Router\\": "src/Router/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JulienLinard\\Router\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-text"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.3.0"
}
